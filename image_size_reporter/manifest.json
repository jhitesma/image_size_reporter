{
    "manifest_version": 3,
    "name": "Image Size Reporter",
    "description": "Report info on all images on a page",
    "version": "0.0.0.1",
    "content_scripts": [
        {
            "js": [
                "scripts/size_reporter.js"
            ],
            "matches": [
                "*://*/*"
            ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
}